Pseudo:
function PrintSeries10_12_14(int numOfTerms)
	term = 10
	for I=1 ; I <= numOfTerms ; I++
		print term + ''
		term =term + 2	
	end for
function TestPrintSeries10_12_14()

	INPUT numOFTerms
	PrintSeries10_12_14(numOfTerms)


------------------------------------------------------------------

Code:

internal class Program
{
    static void PrintSeries10_12_14(int numOfTerms)
    {
        int term = 10;
        for (int I = 1; I <= numOfTerms; I++)
        {
            Console.Write($"{term} ," );
    
            term = term + 2;
        }
    }

    static void TestPrintSeries10_12_14()
    {
        Console.Write("Enter number of terms");
        int numOfTerms = int.Parse(Console.ReadLine());
        Console.Write("Series:");
        PrintSeries10_12_14(numOfTerms);
        Console.WriteLine();
    }

    static void Main(string[] args)
    {
        Console.WriteLine("--------------------Print series-----------------");
        TestPrintSeries10_12_14();
        Console.WriteLine("--------------------Endseries-----------------");
        Console.WriteLine("Press any key to continue");
        Console.ReadKey();
    }
}

